{% trans_default_domain 'FOSUserBundle' %}

<div ng-controller="FormHandlerUserRegister">
	<form ng-submit="submit('{{ path('fos_user_registration_register') }}')" {{ form_enctype(form) }} class="fos_user_registration_register">
	    {{ form_widget(form) }}
	    <div>
	        <input type="submit" value="{{ 'registration.submit'|trans }}" />
	    </div>
	</form>
</div>
